{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "module:react-native-dotenv",
      {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "safe": false,
        "allowUndefined": true,
        "verbose": false
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@": "./src",
          "assets": "./src/assets",
          "components": "./src/components",
          "navigation": "./src/navigation",
          "screens": "./src/screens",
          "styles": "./src/styles",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
